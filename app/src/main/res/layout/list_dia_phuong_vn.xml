<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textViewTenDiaPhuong"
        android:layout_width="82dp"
        android:layout_height="36dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:text="loading..."
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewNhiemTaiDiaPhuong"
        android:layout_width="65dp"
        android:layout_height="21dp"
        android:layout_marginStart="28dp"
        android:text="loading..."
        android:textColor="#FF0000"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewTenDiaPhuong"
        app:layout_constraintStart_toEndOf="@+id/textViewTenDiaPhuong" />

    <TextView
        android:id="@+id/textViewTuVongDiaPhuong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:text="loading..."
        android:textColor="#000000"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewNhiemTaiDiaPhuong"
        app:layout_constraintStart_toEndOf="@+id/textViewNhiemTaiDiaPhuong" />

    <TextView
        android:id="@+id/textViewBinhPhucDiaPhuong"
        android:layout_width="61dp"
        android:layout_height="20dp"
        android:layout_marginStart="76dp"
        android:text="loading..."
        android:textColor="#60BD64"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewTuVongDiaPhuong"
        app:layout_constraintStart_toEndOf="@+id/textViewTuVongDiaPhuong" />
</androidx.constraintlayout.widget.ConstraintLayout>